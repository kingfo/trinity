class asunit.util.ArrayUtil {		public static function indexOf(array:Array, item:Object, fromIndex:Number):Number{		if(fromIndex==undefined) fromIndex = 0;		for (var i : Number = fromIndex; i < array.length; i++) {			if(array[i]==item) return i;		}		return -1;	}		public static function forEach(array:Array, func:Function, thisObject:Object):Void{		if(thisObject==undefined) thisObject = null;		for (var i:Number = 0; i < array.length; i++) {			func.call(thisObject, array[i], i, array);		}	}	public static function every(array:Array, func:Function, thisObject:Object):Boolean{		if(thisObject==undefined) thisObject = null;		for (var i : Number = 0; i < array.length; i++) {			if(!func.call(thisObject, array[i], i, array)) return false;		}		return true;	}}